# Environment variables for FPTI application

# Database
DATABASE_URL=sqlite:///./data/fpti.db

# Market Data APIs
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here
YAHOO_FINANCE_API_KEY=your_yahoo_finance_key_here

# Security
SECRET_KEY=your-secret-key-here-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Application Settings
DEBUG=True
API_V1_STR=/api/v1
PROJECT_NAME=Financial Portfolio Tracking Interface

# External APIs
MARKET_DATA_PROVIDER=yahoo  # Options: yahoo, alpha_vantage
API_RATE_LIMIT_PER_MINUTE=60

# Dashboard Settings
DASH_HOST=127.0.0.1
DASH_PORT=8050
DASH_DEBUG=True